<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Adresse" table="adresse">
    <id name="idadresse" type="bigint" column="idAdresse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="telephone" type="string" column="telephone" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="latitude" type="float" column="latitude" precision="10" scale="0" nullable="true"/>
    <field name="longitude" type="float" column="longitude" precision="10" scale="0" nullable="true"/>
    <field name="boitepostale" type="string" column="boitePostale" length="255" nullable="true"/>
    <field name="lieu" type="string" column="lieu" length="255" nullable="true"/>
    <field name="statut" type="integer" column="statut" nullable="true"/>
  </entity>
</doctrine-mapping>
